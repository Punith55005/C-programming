#include <stdio.h>

int checkAndReturn(int num1, int num2) {
    return (num1 == 30 || num2 == 30 || num1 + num2 == 30);
}

int main() {
    int num1, num2;

    printf("Enter the first integer: ");
    scanf("%d", &num1);

    printf("Enter the second integer: ");
    scanf("%d", &num2);

    if (checkAndReturn(num1, num2)) {
        printf("True\n");
    } else {
        printf("False\n");
    }

    return 0;
}
